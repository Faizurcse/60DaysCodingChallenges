class Sum{
    public static void print( int arr[]){
        int add=0;
        for(int i=0;i<arr.length;i++){
            add=add+arr[i];
        }
        System.out.println("Sum of array:"+add);
    }
    public static void main(String[] args){
        int arr[]={10,20,30};
        print(arr);
    }
}
