import java.util.Scanner;
class table
{
    public static void main(String args[])
    {
      int i,j=1;
      Scanner rf=new Scanner(System.in);
      System.out.println("enter the tbale number");
      i=rf.nextInt();
      while(j<=10)
      {
          System.out.println(i*j);
          j=j+1;
      }
    } 
}
