import java.util.Scanner;
class pros
{
    public static void main(String a[])
    {
        int i,num, add=0;
        Scanner rf=new Scanner(System.in);
        System.out.println("Enter the number");
        num=rf.nextInt();
        i=1;
        while(i<=num)
        {
            add=add+i;
            i=i+1;
            System.out.println(i);

        }
    }
} 
