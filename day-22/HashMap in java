import java.util.HashMap;
public class Map {
    public static void main(String args[]){
        HashMap<String,Integer>fr=new HashMap<>();
        fr.put("India",120);
        fr.put("France",12);
        fr.put("Bangladesh",30);
        fr.put("China",20);
        System.out.println(fr);
        fr.put("India",1200);
        System.out.println("New value of India: "+fr.get("India"));
    }
}
