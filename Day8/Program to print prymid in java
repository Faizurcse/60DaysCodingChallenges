import java.util.Scanner;
public class prymid {
    
    public static void main(String arg[])
    {
        int i,j,n,k=1;
        Scanner rf=new Scanner(System.in);
        System.out.println("Enter the number");
        n=rf.nextInt();
        for(i=n;i>=1;i--)
        {
            for(j=2;j<=i;j++)
            System.out.print(k);
            k=k-1;
            System.out.println();
        }

    }
    
}
