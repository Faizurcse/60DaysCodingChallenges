import java.util.Scanner;
public class star {
    public static void main(String args[])
    {
        int i,j,n;
        Scanner rf=new Scanner(System.in);
        System.out.println("Enter the number");
        n=rf.nextInt();
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=i;j++)
            System.out.print(i);
            System.out.println();
        }
    }
}
