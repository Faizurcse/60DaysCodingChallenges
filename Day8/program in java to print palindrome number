import java.util.Scanner;
class pal
{
    public static void main(String args[])
    {
        int n,rev=0,z;
        Scanner rf=new Scanner(System.in);
        System.out.println("Enter the number");
        n=rf.nextInt();
        z=n;
        while(n>0)
        {
        rev=(rev*10)+n%10;
        n=n/10;
        }
        if(rev==z)
        System.out.println("this number is palindrome"+rev);
        else
        System.out.println("this number is not palindrome"+rev);
        
    }
}
