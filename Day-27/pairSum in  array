import java.util.ArrayList;
public class PairSum {
    public static void main(String args[]){
        ArrayList<Integer> list = new ArrayList<Integer>();
        int a[]={1,2,3,4,5};
        int sum=5;
        for(int i=0;i<a.length;i++){
            for(int j=i+1;j<a.length;j++){
                if(a[i]+a[j]==sum){
                    list.add(a[i]);
                    list.add(a[j]);
                }
            }
        }
        System.out.println(list);
    }
}
